{
  "store.product": {
    "children": [
      "responsive-layout.mobile#product",
      "responsive-layout.desktop#product"
    ]
  },
  "responsive-layout.mobile#product": {
    "children": [
      "flex-layout.row#breadcrumb",
      "flex-layout.row#brand-and-rating",
      "flex-layout.row#product-name",
      "flex-layout.row#product-identifier",
      "flex-layout.row#product-images",
      "flex-layout.row#sku-selector",
      "flex-layout.row#assembly-options",
      "flex-layout.row#divider",
      "flex-layout.row#box",
      "product-kit#buy-together",
      "flex-layout.row#description-and-gallery",
      "flex-layout.row#features",
      "flex-layout.row#specification",
      "check-permission#sticky-buy",
      "flex-layout.row#reviews-and-ratings",
      "flex-layout.row#alsoView"
    ]
  },
  "responsive-layout.desktop#product": {
    "children": [
      "flex-layout.row#product",
      "flex-layout.row#alsoView",
      "flex-layout.row#customer-support",
      "flex-layout.row#bestSellingProducts"
    ]
  },
  "flex-layout.row#product": {
    "children": ["flex-layout.col#product"]
  },
  "flex-layout.col#product": {
    "children": [
      "flex-layout.row#breadcrumb",
      "flex-layout.row#product-details",
      "product-kit#buy-together", // This block won't work if it's not a direct children of store.product
      // "flex-layout.row#description-and-gallery",
      // "flex-layout.row#features",
      // "flex-layout.row#specification",
      // "flex-layout.row#reviews-and-ratings"
      "flex-layout.row#specification"
    ],
    "props": {
      "blockClass": "pdpContent"
    }
  },
  "flex-layout.row#bestSellingProducts": {
    "props": {
      "blockClass": "bestSellingProductsSection"
    },
    "children": ["flex-layout.col#bestSellingProducts"]
  },
  "flex-layout.row#bestSellingProducts--mobile": {
    "props": {
      "blockClass": "bestSellingProductsSection--mobile"
    },
    "children": ["flex-layout.col#bestSellingProducts"]
  },
  "flex-layout.col#bestSellingProducts": {
    "props": {
      "width": "100",
      "blockClass": "bestSellingProducts"
    },
    "children": ["rich-text#bestSellingTitle", "flex-layout.row#bsproducts"]
  },
  "rich-text#bestSellingTitle": {
    "props": {
      "text": "### BEST SELLING PRODUCTS",
      "blockClass": "bestSellingTitle"
    }
  },
  "flex-layout.row#bsproducts": {
    "props": {
      "blockClass": "bsproducts",
      "horizontalAlign": "center",
      "verticalAlign": "center",
      // "preventHorizontalStretch": true,
      // "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#p1",
      "flex-layout.col#p2",
      "flex-layout.col#p3",
      "flex-layout.col#p4"
    ]
  },

  "flex-layout.col#p1": {
    "props": {
      "blockClass": "bsproduct-col",
      "width": "23"
    },
    "children": [
      "image#p1",
      "rich-text#shopNow1",
      "rich-text#t1",
      "rich-text#d1"
    ]
  },
  "image#p1": {
    "props": {
      "blockClass": "bsp-image",
      "src": "https://demo.engineering-edge.com/assets/img/corroshield/cs-product-1.webp",
      "maxHeight": 400
    }
  },
  "rich-text#shopNow1": {
    "props": {
      "text": "[SHOP NOW](/)",
      "blockClass": "bsp-cta",
      "textAlignment": "CENTER"
    }
  },
  "rich-text#t1": {
    "props": {
      "text": "Skylight Fixing Screws",
      "blockClass": "bsp-heading",
      "textAlignment": "CENTER"
    }
  },
  "rich-text#d1": {
    "props": {
      "text": "Designed for crest fixing fibreglass skylight roof sheets to steel purlins",
      "blockClass": "bsp-desc",
      "textAlignment": "CENTER"
    }
  },
